					<requirement name="NotHasBuff" target="other" buff="vehicleSpiderRiderBikeCheck"/>
				</triggered_effect>			
			</effect_group>				
		</buff>

		<buff name="vehicleSpiderRiderBikeCheck" hidden="true">
